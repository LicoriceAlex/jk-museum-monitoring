{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "row",
      "title": "CPU & Memory",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1000
    },
    {
      "id": 1,
      "title": "CPU usage, %",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (name) (rate(container_cpu_usage_seconds_total{image!=\"\", name=~\"$container\"}[$__rate_interval])) * 100",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "id": 2,
      "title": "Memory working set, MB",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
      "fieldConfig": {
        "defaults": { "unit": "megabytes", "min": 0 },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (name) (container_memory_working_set_bytes{image!=\"\", name=~\"$container\"}) / 1024 / 1024",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "id": 3,
      "title": "Memory usage, % of limit",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "fieldConfig": {
        "defaults": { "unit": "percent", "min": 0, "max": 100 },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(sum by (name) (container_memory_working_set_bytes{image!=\"\", name=~\"$container\"}) / clamp_min(sum by (name) (container_spec_memory_limit_bytes{image!=\"\", name=~\"$container\"}), 1)) * 100",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "id": 4,
      "title": "Container filesystem usage, GiB",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "fieldConfig": {
        "defaults": { "unit": "gibibytes", "min": 0 },
        "overrides": []
      },
      "options": {
        "legend": { "showLegend": true, "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (name) (container_fs_usage_bytes{name=~\"$container\", image!=\"\"}) / 1024 / 1024 / 1024",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },

    {
      "type": "row",
      "title": "Disk & Network IO",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
      "id": 1001
    },
    {
      "id": 5,
      "title": "Disk reads, bytes/s",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 18 },
      "fieldConfig": { "defaults": { "unit": "Bps", "min": 0 }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (name) (rate(container_fs_reads_bytes_total{image!=\"\", name=~\"$container\"}[$__rate_interval]))",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "id": 6,
      "title": "Disk writes, bytes/s",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 18 },
      "fieldConfig": { "defaults": { "unit": "Bps", "min": 0 }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (name) (rate(container_fs_writes_bytes_total{image!=\"\", name=~\"$container\"}[$__rate_interval]))",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "id": 7,
      "title": "Net receive, bytes/s",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 25 },
      "fieldConfig": { "defaults": { "unit": "Bps", "min": 0 }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (name) (rate(container_network_receive_bytes_total{image!=\"\", name=~\"$container\"}[$__rate_interval]))",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },
    {
      "id": 8,
      "title": "Net transmit, bytes/s",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 25 },
      "fieldConfig": { "defaults": { "unit": "Bps", "min": 0 }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (name) (rate(container_network_transmit_bytes_total{image!=\"\", name=~\"$container\"}[$__rate_interval]))",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    },

    {
      "type": "row",
      "title": "Lifecycle",
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 1002
    },
    {
      "id": 9,
      "title": "Container uptime, hours",
      "type": "timeseries",
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 33 },
      "fieldConfig": { "defaults": { "unit": "h", "min": 0 }, "overrides": [] },
      "options": { "legend": { "showLegend": true, "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        {
          "refId": "A",
          "expr": "(time() - max by (name) (container_start_time_seconds{image!=\"\", name=~\"$container\"})) / 3600",
          "legendFormat": "{{name}}",
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["containers", "cadvisor", "docker"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "current": { "text": "Prometheus", "value": "Prometheus" },
        "hide": 2
      },
      {
        "name": "container",
        "label": "Container (name)",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(container_cpu_usage_seconds_total{image!=\"\"}, name)",
        "includeAll": true,
        "multi": true,
        "refresh": 2,
        "sort": 1,
        "current": { "text": "All", "value": ".*" },
        "regex": "",
        "definition": ""
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s","10s","30s","1m","5m","15m"] },
  "timezone": "",
  "title": "Container Metrics (cAdvisor)",
  "uid": "container-metrics-cadvisor",
  "version": 1,
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "12.1.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "2.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" }
  ]
}
